data.location=~/Sync/Files/Tasks

urgency.age.coefficient=10.0
urgency.uda.priority.L.coefficient=-3.0
urgency.user.tag.codogo.coefficient=3.0

uda.reviewed.type=date
uda.reviewed.label=Reviewed

report._reviewed.description=Tasksh review report.  Adjust the filter to your needs.
report._reviewed.columns=uuid
report._reviewed.sort=reviewed+,modified+
report._reviewed.filter=( reviewed.none: or reviewed.before:now-6days ) and ( +PENDING or +WAITING )

report.bar.columns=id,project,description
report.bar.filter=+ACTIVE or +OVERDUE
report.bar.sort=urgency-

report.pressing.columns=urgency,id,description,start.age,due.relative,entry.age,depends,priority,project,tags,recur,scheduled.countdown,until.remaining
report.pressing.filter=+ACTIVE or +OVERDUE or +DUE
report.pressing.sort=urgency-

report.shopping.filter=project:shopping
report.shopping.columns=urgency,id,description,start.age,entry.age,depends,priority,project,tags,recur,scheduled.countdown,due.relative,until.remaining
report.pressing.filter=+ACTIVE or +OVERDUE or +DUE
report.shopping.sort=urgency-

report.next.columns=urgency,id,description,start.age,due.relative,entry.age,depends,priority,project,tags,recur,scheduled.countdown,until.remaining
report.next.labels=
#report.next.labels=Urg,ID,Description,Active,Age,Due,Deps,P,Project,Tag,Recur,S,Until
report.next.filter=-BLOCKED status:pending limit:page


