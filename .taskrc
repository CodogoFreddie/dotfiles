include ~/.config/taskwarrior/default.report
include ~/.config/taskwarrior/inbox.report
include ~/.config/taskwarrior/journal.report
include ~/.config/taskwarrior/next.report
include ~/.config/taskwarrior/reviewed.report
include ~/.config/taskwarrior/shopping.report
include ~/.config/taskwarrior/simple.report
include ~/.config/taskwarrior/theme
include ~/.config/taskwarrior/udas
include ~/.config/taskwarrior/urgency

alias.jot = add +inbox due:1d
alias.snooze = modify snooze:1h

context.couple = +drewer
context.dazzlebyte = project:dzb
context.lbz = project:lbz
context.solo = -drewer project:

data.location = ~/.task

default.command = default
