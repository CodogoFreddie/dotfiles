#define  BLACK        #2c292d
#define  RED          #ff6188
#define  GREEN        #a9dc76
#define  YELLOW       #ffd866
#define  BLUE         #78dce8
#define  PURPLE       #ab9df2
#define  ORANGE       #fc9867
#define  WHITE        #fdf9f3
#define  DARK_BLACK   #160d16
#define  DARK_RED     #ff2e62
#define  DARK_GREEN   #90ee31
#define  DARK_YELLOW  #ffcb33
